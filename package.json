{
	"name": "easy-web-share",
	"version": "1.0.1",
	"type": "commonjs",
	"exports": {
		".": {
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			}
		}
	},
	"main": "./dist/index.umd.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": ["dist"],
	"scripts": {
		"build": "vite build",
		"format": "biome format --write ./"
	},
	"keywords": [
		"h5 share",
		"web share",
		"native share",
		"QQ空间分享",
		"微信分享"
	],
	"author": "onelong",
	"repository": {
		"type": "git",
		"url": "https://github.com/lbb00/easy-web-share"
	},
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^1.7.3",
		"terser": "^5.31.0",
		"typescript": "^5.4.5",
		"vite": "^5.2.11",
		"vite-plugin-dts": "^3.9.1"
	}
}
